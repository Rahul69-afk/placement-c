#include <stdio.h>

int main() {
    int n, sum = 0;

    // Read number of elements
    scanf("%d", &n);
    int a[n];

    // Read array elements and calculate sum
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        sum += a[i];
    }

    // Output the sum
    printf("%d\n", sum);

    return 0;
}
