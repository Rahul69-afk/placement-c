#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int a[n], result[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    for (int i = 0; i < n; i++) {
        if (i == 0) {
            result[i] = a[i] * a[i + 1]; 
        } else if (i == n - 1) {
            result[i] = a[i] * a[i - 1];  
        } else {
            result[i] = a[i - 1] * a[i + 1];  
        }
    }

    for (int i = 0; i < n; i++) {
        printf("%d ", result[i]);
    }
    printf("\n");

    return 0;
}