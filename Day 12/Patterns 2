#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    if (n > 0) {
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                printf("*");
            }
            printf("\n");
        }
    } else {
        printf("Input must be greater than 0.\n");
    }

    return 0;
}
